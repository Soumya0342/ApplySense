{
  "manifest_version": 3,
  "name": "ApplySense",
  "version": "0.1.0",
  "description": "AI-powered LinkedIn job application duplicate detector",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["content.js"]
    }
  ]
}
